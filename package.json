	"scripts": {
		"start": "start-storybook -p 9009 -s public",
		"start-cra": "react-scripts start",
		"build-cra": "react-scripts build ",
		"build": "rm -rf dist/* && NODE_ENV=production babel src/lib --out-dir dist --copy-files  --source-maps both --ignore *.stories.js,__tests__,spec.js,test.js,__snapshots__ && yarn run copypackage",
		"test": "react-scripts test --env=jsdom",
		"eject-dont-do": "react-scripts eject",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public",
		"cy": "cypress open",
		"cy-run": "cypress run",
		"copypackage": "cp -rf package.json dist",
		"dist": "cd dist && bash -c 'npm publish'",
		"linkdist": "cd dist && bash -c 'yarn link'",
		"unlinkdist": "cd dist && bash -c 'yarn unlink'",
		"testls": "npm config ls"
